{
  "name": "beachvollleyball-scoreboard",
  "description": "Scoreboard for the Lazy Volleyball Referee",
  "version": "0.0.1",
  "private": true,
  "main": "app.js",
  "engines": {
    "node": "6.4.0"
  },
  "xo": {
    "global": [
      "describe",
      "it"
    ],
    "ignores": [
      "test/**"
    ],
    "extends": [
      "xo/esnext",
      "xo-react"
    ],
    "rules": {
      "jsx-sort-props": 0
    }
  },
  "dependencies": {
    "babel-cli": "^6.14.0",
    "babel-core": "^6.14.0",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-react": "^6.11.1",
    "babelify": "7.3.0",
    "bluebird": "^3.4.6",
    "body-parser": "1.15.2",
    "bootstrap": "3.3.7",
    "browserify": "13.1.0",
    "bulma": "^0.4.0",
    "const": "1.0.0",
    "coveralls": "2.11.12",
    "del": "2.2.2",
    "envify": "3.4.1",
    "es5-shim": "4.5.9",
    "es6-shim": "0.35.1",
    "express": "4.14.0",
    "gulp": "3.9.1",
    "gulp-babel": "6.1.2",
    "gulp-istanbul": "1.1.1",
    "gulp-mocha": "3.0.1",
    "gulp-notify": "2.2.0",
    "gulp-sourcemaps": "1.6.0",
    "isparta": "4.0.0",
    "istanbul-harmony": "0.3.16",
    "next": "^2.0.0-beta",
    "node-jsx": "0.13.3",
    "react": "15.3.1",
    "react-bootstrap": "0.30.3",
    "react-color": "^2.11.4",
    "react-dom": "^15.3.1",
    "react-tap-event-plugin": "1.0.0",
    "reactify": "1.1.1",
    "should": "11.1.0",
    "sinon": "1.17.5",
    "socket.io": "^1.4.8",
    "sol-style-checker": "1.2.0",
    "styled-components": "^1.4.4",
    "superagent": "2.2.0",
    "url": "^0.11.0",
    "vinyl-source-stream": "1.1.0",
    "winston": "2.2.0"
  },
  "scripts": {
    "test": "gulp test",
    "posttest": "cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js | rm -rf ./coverage",
    "prestart": "next build",
    "start": "node app.js",
    "dev": "next"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/nvbf/beachvollleyball-scoreboard"
  },
  "devDependencies": {
    "eslint-config-xo-react": "^0.11.0",
    "eslint-plugin-react": "^6.10.2",
    "xo": "^0.18.0"
  }
}
