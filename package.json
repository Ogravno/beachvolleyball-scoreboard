  "version": "0.0.1",
  "private": true,
  "main": "app.js",
  "engines": {
    "node": "6.4.0"
  },
  "xo": {
    "global": [
      "describe",
      "it"
    ],
    "ignores": [
      "test/**"
    ],
    "extends": [
      "xo/esnext",
      "xo-react"
    ],
    "rules": {
      "jsx-sort-props": 0
    }
  },
  "dependencies": {
    "babel-cli": "^6.14.0",
    "babel-core": "6.14.0",
    "babel-loader": "6.2.5",
    "babel-polyfill": "6.13.0",
    "babel-preset-es2015": "6.14.0",
    "babel-preset-react": "6.11.1",
    "babel-preset-stage-0": "6.5.0",
    "babelify": "7.3.0",
    "bluebird": "3.4.6",
    "body-parser": "1.15.2",
    "bootstrap": "3.3.7",
    "browserify": "13.1.0",
    "bulma": "^0.4.0",
    "const": "1.0.0",
    "express": "4.14.0",
    "gulp": "3.9.1",
    "gulp-babel": "6.1.2",
    "gulp-istanbul": "1.1.1",
    "gulp-mocha": "3.0.1",
    "gulp-notify": "2.2.0",
    "gulp-sourcemaps": "1.6.0",
    "immutable": "3.8.1",
    "isparta": "4.0.0",
    "istanbul-harmony": "0.3.16",
    "next": "^2.0.0-beta",
    "node-jsx": "0.13.3",
    "react": "15.3.1",
    "react-bootstrap": "0.30.8",
    "react-color": "^2.11.4",
    "react-dom": "^15.3.1",
    "react-redux": "4.4.5",
    "react-router": "2.8.0",
    "react-router-redux": "4.0.5",
    "react-tap-event-plugin": "1.0.0",
    "react-toggle": "^3.0.1",
    "reactify": "1.1.1",
    "redux": "3.6.0",
    "redux-devtools": "^3.3.2",
    "redux-devtools-dock-monitor": "^1.1.1",
    "redux-devtools-extension": "^2.13.0",
    "redux-devtools-log-monitor": "^1.0.11",
    "redux-immutable": "^3.0.8",
    "redux-undo": "^1.0.0-beta9-8",
    "redux-undo-immutable": "^0.2.0",
    "should": "11.1.0",
    "sinon": "1.17.5",
    "socket.io": "1.4.8",
    "styled-components": "^1.4.4",
    "superagent": "2.2.0",
    "webpack": "2.1.0-beta.25",
    "webpack-dev-server": "1.16.1",
    "winston": "2.2.0"
  },
  "scripts": {
    "prestart": "next build",
    "start": "node app.js",
    "url": "^0.11.0",
    "vinyl-source-stream": "1.1.0",
    "winston": "2.2.0",
    "dev": "next",
    "test": "xo --fix && jest",
    "jest:watch": "npm test -- --watch"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/nvbf/beachvollleyball-scoreboard"
  },
  "devDependencies": {
    "babel-jest": "^19.0.0",
    "eslint-config-xo-react": "^0.11.0",
    "eslint-plugin-react": "^6.10.2",
    "jest": "^19.0.2",
    "xo": "^0.18.0"
