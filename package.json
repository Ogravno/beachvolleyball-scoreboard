{
  "name": "beachvollleyball-scoreboard",
  "description": "Scoreboard for the Lazy Volleyball Referee",
  "version": "0.0.1",
  "private": true,
  "main": "app.js",
  "engines": {
    "node": "8"
  },
  "xo": {
    "global": [
      "describe",
      "it"
    ],
    "ignores": [
      "test/**"
    ],
    "extends": [
      "xo/esnext",
      "xo-react"
    ],
    "rules": {
      "jsx-sort-props": 0
    }
  },
  "dependencies": {
    "async-reactor": "^1.2.0",
    "babel-cli": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babelify": "7.3.0",
    "bluebird": "^3.5.0",
    "body-parser": "^1.17.1",
    "bootstrap": "3.3.7",
    "browserify": "14.3.0",
    "bulma": "^0.4.0",
    "chrome-remote-interface": "^0.23.3",
    "const": "1.0.0",
    "cucumber": "^2.3.1",
    "debug": "^2.6.8",
    "express": "4.15.2",
    "firebase": "^4.1.1",
    "immutable": "3.8.1",
    "isomorphic-parse": "0.0.3",
    "material-ui": "^0.18.3",
    "moment": "^2.18.1",
    "next": "latest",
    "preact": "^8.1.0",
    "preact-compat": "^3.16.0",
    "printf": "^0.2.5",
    "react": "15.5.4",
    "react-bootstrap": "0.31.0",
    "react-color": "^2.11.4",
    "react-dom": "15.5.4",
    "react-icons-kit": "^1.0.7",
    "react-overdrive": "0.0.5",
    "react-sidenav": "^2.1.1",
    "react-toggle": "^4.0.1",
    "sendgrid": "^4.10.0",
    "socket.io": "2.0.1",
    "styled-components": "^1.4.4",
    "superagent": "3.5.2",
    "tide": "^2.0.0-alpha.2"
  },
  "scripts": {
    "start": "node app.js",
    "url": "^0.11.0",
    "vinyl-source-stream": "1.1.0",
    "dev": "node app.js",
    "build": "next build",
    "test": "jest",
    "jest:watch": "npm test -- --watch",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "heroku-postbuild": "next build"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/nvbf/beachvollleyball-scoreboard"
  },
  "devDependencies": {
    "@storybook/react": "3.0.0-rc.2",
    "eslint-config-xo-react": "^0.12.0",
    "eslint-plugin-react": "^7.0.1",
    "jest": "^20.0.4",
    "nightwatch": "^0.9.16",
    "prettier-check": "^1.0.0",
    "storybook": "0.0.0",
    "xo": "^0.18.0"
  }
}
